

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Give the name for the following compound:
\begin{center}
\chemfig{CH_3-CH_2-CH(-[:90]CH_2-CH_2-CH_3)-CH_2-CH_3}
\end{center}
\end{question}\begin{solution}
3-ethylpentane
 \end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Give the name for the following compound:
\begin{center}
\chemfig{CH_3-CH(-[:-90]CH_2-CH_3)-CH_2-CH_3}
\end{center}
\end{question}\begin{solution}
3-methylpentane
 \end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%

 %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Give the name for the following compound:
\begin{center}
\chemfig{CH_3-C(-[:90]CH_3)(-[:-90]CH_2-CH_3)-CH_3}
\end{center}
\end{question}\begin{solution}
2,2-dimethylbutane
 \end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%




%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Give the name for the following compound:
\begin{center}
\chemfig{CH_3-CH_2-CH(-[:90]CH_2-CH_3)-CH_2-CH_2-CH_3}
\end{center}
\end{question}\begin{solution}
3-ethylhexane
 \end{solution}\stepcounter{numA}
