\begin{center}\begin{tabular}[t]{  c c    }
\toprule
  M	&$\Pi$ (torr) \\
\midrule
 0.010	&223 	 \\
 0.011	& 245	 \\
 0.012	&267 	 \\
 0.013	&290  \\
  0.014	&312 	 \\
\bottomrule
\end{tabular}\end{center}
\end{question}
