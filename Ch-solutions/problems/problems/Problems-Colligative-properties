 %%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
From the following solutions:
\begin{inparaenum}[(i)]
\item     0.1m \ce{KCl}  in water
\item     0.2m \ce{NaCl}  in water
\item     0.3m \ce{KOH}  in water
\end{inparaenum}
select the one with:
\begin{inparaenum}[(a)]
\item     lowest freezing point   % 0.1m \ce{KCl}  in water
\item     highest freezing point  %0.3m \ce{KOH}  in water
\item     highest boiling point     %0.1m \ce{KCl}  in water
\item     lowest boiling point      %0.3m \ce{KOH}  in water
\item     lowest vapor pressure  %0.1m \ce{KCl}  in water
 \item    highest vapor pressure  %0.3m \ce{KOH}  in water
 \item    highest osmotic pressure %0.1m \ce{KCl}  in water
 \item    lowest osmotic pressure %0.3m \ce{KOH}  in water
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item       0.1m \ce{KCl}  in water
\item     0.3m \ce{KOH}  in water
\item     0.1m \ce{KCl}  in water
\item      0.3m \ce{KOH}  in water
\item      0.1m \ce{KCl}  in water
 \item     0.3m \ce{KOH}  in water
 \item     0.1m \ce{KCl}  in water
 \item     0.3m \ce{KOH}  in water
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

 
 
 
 
